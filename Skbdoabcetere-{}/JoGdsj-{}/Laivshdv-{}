    AutoSell = {
        Sell_Enabled = false,
        Sell_Common = false,
        Sell_Rare = false,
        Sell_Legendary = false,
        Sell_Mythical = false,
        Sell_Ultimate = false
    },
    AutoBank = {
    	Transfer_Enabled = false,
        Transfer_Common = false,
        Transfer_Rare = false,
        Transfer_Legendary = false,
        Transfer_Mythical = false,
        Transfer_Ultimate = false
    },
    AutoEquip = {
        Emblem = false,
    },
    AutoLeave = {
        WhenDied = {
            Enabled = false,
            Delay = 2
        },
        WhenInventoryFull = {
            Enabled = false,
            Delay = 2
        }
    }
}

