    Title = "Delay Start",
    Default = DelayStart,
    Placeholder = "Delay Number Here",
    Numeric = false,
    Finished = false,
    Callback = function(Valueinf)
        DelayStart = tonumber(Valueinf) or 1
    end
})

local AutoC = Tabs.Main:AddToggle("AutoCreate", {Title = "Auto Create Squad", Default = false })

    AutoC:OnChanged(function(ACR)
        while true and ACR do
        wait(DelayStart)
        createsquad()
        end
    end)
    
