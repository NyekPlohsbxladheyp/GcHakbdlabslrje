local vu = game:GetService("VirtualUser")
repeat wait() until game:IsLoaded() 
	game:GetService("Players").LocalPlayer.Idled:connect(function()
    game:GetService("VirtualUser"):ClickButton2(Vector2.new())
		vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		wait(1)
		vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
     end)
end
end

if Playtime_2 then
if questslot1 == "Playtime" or questslot2 == "Playtime" then
writefile("UnknownHubXUniversal/TreasureQuest/AutoQuestConfig.txt", "NormalQuest")
createsquad()
wait(3)
startsquad()
end
end

if questslot1 == "Shopper" or questslot2 == "Shopper" then
    local DailyShop = require(game:GetService("Players").LocalPlayer.PlayerScripts.Shared.Modules.Pages.DailyShop.Equipment)
    local GetNumbers = {""}

    for i = 1, 100 do
        table.insert(GetNumbers, i)
    end

    for _, index in pairs(GetNumbers) do
        -- Check for notification
        local coreGui = game:GetService("CoreGui")
        local robloxGui = coreGui:FindFirstChild("RobloxGui")
        local notificationFrame = robloxGui and robloxGui:FindFirstChild("NotificationFrame")

        if notificationFrame then
            local descendants = notificationFrame:GetDescendants()
            local notificationFound = false
            
            for _, descendant in ipairs(descendants) do
                if descendant.Name == "Notification" then
                    local notificationImage = descendant:FindFirstChild("NotificationImage")
                    if notificationImage and notificationImage.Image == "rbxassetid://3200915096" then
                        notificationFound = true
                        print("Notification detected, stopping script.")
                        break
                    end
                end
            end
            
            if notificationFound then
                break
            end
        end

        -- Proceed with item purchase if no notification is found
        local item = DailyShop["_item_slots"][index]
        if item and item.ItemData then
            local id = item.ItemData.ItemID
            local rarity = item.DatabaseInfo.Rarity

            print("Attempting to buy item with ID:", id)

            local ShopBuy = {
                [1] = "id"
            }
            
        if rarity == "Common" or rarity == "Rare" or rarity "Legendary" then
            ShopBuy[1] = id
       end
     
            wait(2)

            game:GetService("ReplicatedStorage").Lobby.Remotes.BuyEquipment:FireServer(unpack(ShopBuy))
        else
            print("No item found at index:", index)
        end
    end
end
end)
end

