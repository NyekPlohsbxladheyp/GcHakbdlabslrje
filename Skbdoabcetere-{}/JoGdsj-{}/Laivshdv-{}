function skipmissingquest()
if questslot1== "Shopper" or questslot1 == "Blacksmith" or questslot1 == "Magic Merchant" or questslot1 == "Use Scrolls" or questslot1 == "Power Scroll" or questslot1 == "Rename Item" then
game:GetService("ReplicatedStorage").Shared.Remotes.Data.SkipQuest:FireServer(1)
end

if questslot2== "Shopper" or questslot2 == "Blacksmith" or questslot2 == "Magic Merchant" or questslot2 == "Use Scrolls" or questslot2 == "Power Scroll" or questslot2 == "Rename Item" then
game:GetService("ReplicatedStorage").Shared.Remotes.Data.SkipQuest:FireServer(2)
end
end

