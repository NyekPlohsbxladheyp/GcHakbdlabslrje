Tabs.Main:AddParagraph({
        Title = "Auto-Sell item",
        Content = "Pls Becareful u will lose ur items"
    })

-- Function to auto-sell items based on rarity settings
local itemToSell = {}

function autoSell()
    if not _G.Setting.AutoSell.Sell_Enabled then
        return
    end

    for _, item in pairs(InventoryController.Inventory.Items) do
        local id = item.Data.ItemID
        local rarity = item.DatabaseInfo.Rarity

        if _G.Setting.AutoSell.Sell_Common and rarity == 'Common' then
            table.insert(itemToSell, id)
        end

        if _G.Setting.AutoSell.Sell_Rare and rarity == 'Rare' then
            table.insert(itemToSell, id)
        end

        if _G.Setting.AutoSell.Sell_Legendary and rarity == 'Legendary' then
            table.insert(itemToSell, id)
        end

        if _G.Setting.AutoSell.Sell_Mythical and rarity == 'Mythical' then
            table.insert(itemToSell, id)
        end

        if _G.Setting.AutoSell.Sell_Ultimate and rarity == 'Ultimate' then
            table.insert(itemToSell, id)
        end
    end

print('Selling Items:', table.unpack(itemToSell))
    -- Example: FireServer with the itemToSell table
    game:GetService("ReplicatedStorage").Lobby.Remotes.Sell:FireServer(itemToSell)
end

-- Example Toggle creation for auto-sell (Adjust according to your UI library)
function toggleCallback(toggleName, value)
    _G.Setting.AutoSell[toggleName] = value
    
if value then
    spawn(autoSell)  -- Call autoSell immediately when a toggle changes
    end
end

local Toggle = Tabs.Main:AddToggle("AutoSell", {Title = "Auto Sell", Default = false })

    Toggle:OnChanged(function(Value)
        toggleCallback("Sell_Enabled", Value)
    end)
    
Options.AutoSell:SetValue(false)
    
local Toggle = Tabs.Main:AddToggle("Sell_Common", {Title = "Sell Common", Default = false })

    Toggle:OnChanged(function(Value)
        toggleCallback("Sell_Common", Value)
    end)

local Toggle = Tabs.Main:AddToggle("Sell_Rare", {Title = "Sell Rare", Default = false })

    Toggle:OnChanged(function(Value)
        toggleCallback("Sell_Rare", Value)
    end)

local Toggle = Tabs.Main:AddToggle("Sell_Legendary", {Title = "Sell Legendary", Default = false })

    Toggle:OnChanged(function(Value)
        toggleCallback("Sell_Legendary", Value)
    end)

local Toggle = Tabs.Main:AddToggle("Sell_Mythical", {Title = "Sell Mythical", Default = false })

    Toggle:OnChanged(function(Value)
        toggleCallback("Sell_Mythical", Value)
    end)

local Toggle = Tabs.Main:AddToggle("Sell_Ultimate", {Title = "Sell Ultimate", Default = false })

    Toggle:OnChanged(function(Value)
        toggleCallback("Sell_Ultimate", Value)
    end)    

Tabs.Main:AddParagraph({
        Title = "Auto Bank",
        Content = "Warning: Auto Bank Can Transfer Equipped Weapon"
    })

function autoBank()
    while _G.Setting.AutoBank.Transfer_Enabled do
        local itemToBank = {
            [1] = "Inventory",
            [2] = "id", 
            [3] = 1
        }

        for _, item in pairs(InventoryController.Inventory.Items) do
            local id = item.Data.ItemID
            local rarity = item.DatabaseInfo.Rarity

            if _G.Setting.AutoBank.Transfer_Common and rarity == 'Common' then
                itemToBank[2] = id
            end

            if _G.Setting.AutoBank.Transfer_Rare and rarity == 'Rare' then
                itemToBank[2] = id
            end

            if _G.Setting.AutoBank.Transfer_Legendary and rarity == 'Legendary' then
                itemToBank[2] = id
            end

            if _G.Setting.AutoBank.Transfer_Mythical and rarity == 'Mythical' then
                itemToBank[2] = id
            end

            if _G.Setting.AutoBank.Transfer_Ultimate and rarity == 'Ultimate' then
                itemToBank[2] = id
            end
        end

        print('Bank Transfer Item:', itemToBank[2])

        
        game:GetService("ReplicatedStorage").Lobby.Remotes.BankTransfer:FireServer(unpack(itemToBank))

        wait(0.5)
    end
end

function toggleBankCallback(toggleName, value)
    _G.Setting.AutoBank[toggleName] = value

    if value then
        spawn(autoBank)
    end
end

local BankToggle = Tabs.Main:AddToggle("AutoBank", {Title = "Auto Transfer", Default = false })

BankToggle:OnChanged(function(Value)
    toggleBankCallback("Transfer_Enabled", Value)
end)

local CommonToggle = Tabs.Main:AddToggle("AutoBankCommon", {Title = "Transfer Common", Default = false })

CommonToggle:OnChanged(function(Value)
    toggleBankCallback("Transfer_Common", Value)
end)

local RareToggle = Tabs.Main:AddToggle("AutoBankRare", {Title = "Transfer Rare", Default = false })

RareToggle:OnChanged(function(Value)
    toggleBankCallback("Transfer_Rare", Value)
end)

local LegendaryToggle = Tabs.Main:AddToggle("AutoBankLegendary", {Title = "Transfer Legendary", Default = false })

LegendaryToggle:OnChanged(function(Value)
    toggleBankCallback("Transfer_Legendary", Value)
end)

local MythicalToggle = Tabs.Main:AddToggle("AutoBankMythical", {Title = "Transfer Mythical", Default = false })

MythicalToggle:OnChanged(function(Value)
    toggleBankCallback("Transfer_Mythical", Value)
end)

local UltimateToggle = Tabs.Main:AddToggle("AutoBankUltimate", {Title = "Transfer Ultimate", Default = false })

UltimateToggle:OnChanged(function(Value)
    toggleBankCallback("Transfer_Ultimate", Value)
end)

-- Quest

Tabs.Quest:AddParagraph({
        Title = "Auto Quest",
        Content = " "
    })

local AutoQuestToggle = false
local skipquest = false

local Toggle = Tabs.Quest:AddToggle("AutoQuest", {Title = "Auto Quest", Default = false })

    Toggle:OnChanged(function(QuestValue)
    AutoQuestToggle = QuestValue
 while AutoQuestToggle and wait(5) do
 if not AutoQuestToggle then break end
 if skipquest then
 skipmissingquest()
 end
 if not playtime_3 == true then
 AutoQuestCombo()
 wait(3)
 StartAutoQuest2()
wait(5)
 AutoQuest_2()
 wait(0.1)
 AutoQuest_1()
 wait(0.1)
StartAutoQuest()
end
if playtime_3 then
print("Auto Quest While Playtime Mode")
AutoQuest_2()
AutoQuest_1()
wait(3)
StartAutoQuest()
 end
 end
end)

Tabs.Quest:AddInput("QuestDiffculty", {
    Title = "Quest Difficulty",
    Default = QuestDiffculty,
    Placeholder = "Enter a Difficulty Pls",
    Numeric = false,
    Finished = false,
    Callback = function(Quest_1)
        QuestDiffculty = tostring(Quest_1) or "Easy"
    end
})

Tabs.Quest:AddParagraph({
        Title = "Playtime Quest",
        Content = " "
    })
    
local Dropdown = Tabs.Quest:AddDropdown("PlaytimeMode", {
        Title = "Playtime Quest",
        Values = {"AFK Lobby Mode", "Custom Farm Mode", "Auto Quest While Playtime"},
        Multi = false,
        Default = 1,
    })
 
Dropdown:OnChanged(function(Value2)
if Value2 == "AFK Lobby Mode" then
        Playtime_1 = Value2
        Playtime_2 = false
        Playtime_3 = false
 elseif Value2 == "Custom Farm Mode" then
        Playtime_1 = false
		Playtime_2 = Value2
		Playtime_3 = false
elseif Value2 == "Auto Quest While Playtime" then
		Playtime_1 = false
		Playtime_2 = false
		Playtime_3 = Value2       
		end
    end)
    
Tabs.Quest:AddParagraph({
        Title = "Secret Shop Quest",
        Content = " "
    })

local Dropdown = Tabs.Quest:AddDropdown("SecretShopMode", {
        Title = "Secret Shop Quest",
        Values = {"Coral Kingdom", "Daring Dojo", "Fiery Fortress"},
        Multi = false,
        Default = 1,
    })
 
Dropdown:OnChanged(function(Value)
if Value == "Coral Kingdom" then
        SecretShop_1 = Value
        SecretShop_2 = false
        SecretShop_3 = false
elseif Value == "Daring Dojo" then
		SecretShop_1 = false
        SecretShop_2 = Value
        SecretShop_3 = false
elseif Value == "Fiery Fortress" then
		SecretShop_1 = false
        SecretShop_2 = false
        SecretShop_3 = Value
		end
    end)

Tabs.Quest:AddParagraph({
        Title = "Toggles",
        Content = " "
    })

local skipquest = false

local AutoSkip = Tabs.Quest:AddToggle("AutoSkipQuest", {Title = "Auto Skip When Missing Quest", Default = false })

AutoSkip:OnChanged(function(Value)
    skipquest = Value
end)

